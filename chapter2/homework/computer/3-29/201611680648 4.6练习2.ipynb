    "def cute(n):\n",
    "    i=1\n",
    "    j=0\n",
    "    total=0\n",
    "    while j<n:\n",
    "        j=j+1\n",
    "        total=total+(1/i)*(-1)**(j+1)\n",
    "        i+=1\n",
    "    return total\n",
    "n=1000\n",
    "print(4*cute(n))\n",
    "n=10000\n",
    "print(4*cute(n))"
   ]
  }
